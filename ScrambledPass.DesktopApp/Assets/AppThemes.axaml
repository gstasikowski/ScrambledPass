<ResourceDictionary xmlns="https://github.com/avaloniaui"
		xmlns:controls="using:RatingControlSample.Controls"
		xmlns:converter="using:RatingControlSample.Converter"
				xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Light">
			<SolidColorBrush x:Key="PasswordQualityGreat" Color="Green"/>
			<SolidColorBrush x:Key="PasswordQualityGood" Color="Blue"/>
			<SolidColorBrush x:Key="PasswordQualityWeak" Color="Orange"/>
			<SolidColorBrush x:Key="PasswordQualityPoor" Color="Red"/>
		</ResourceDictionary>
		<ResourceDictionary x:Key="Dark">
			<SolidColorBrush x:Key="PasswordQualityGreat" Color="Green"/>
			<SolidColorBrush x:Key="PasswordQualityGood" Color="CornflowerBlue"/>
			<SolidColorBrush x:Key="PasswordQualityWeak" Color="Yellow"/>
			<SolidColorBrush x:Key="PasswordQualityPoor" Color="OrangeRed"/>
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>